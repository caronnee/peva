ROBOT a1
{
	HEALTH = 10
	ATTACK = 33			
	Missilles = 10
	Angle = 45

	memory = 100
	MHEALTH = 100
	Mattack  = 40
	KILL a3

	Object o = NULL;
	main()
	{
		for(integer i = 0; i <6;i++)
		{
			integer s = see();
			for (integer i =0 ;i < s; i++)
			{
				if (isPlayer(eye[i]))
				{
					o = eye[i];
					break;
				}
				else
				{
					turn(180);
					move(5*i+5);
				}
			}
			while (hit(o) > 0 )
			{
				shoot(direction(locate(o)));
			}
		}
	}
}
