ROBOT qqq
{
	HEALTH = 100
	ATTACK = 33			
	SEE_WIDTH = 8
	SEE_HEIGHT = 4
	SEE = 56
        DEFENSE	= 22		
	MATTACK = 50	
	MHEALTH	= 20	
        ID = 5			

	KILLED < 5
	VISIT_SEQ( START[3], START[2], [13,18])
	VISIT( [100,200])

	location l = {100,200};
	main() // komentar
	{
		shoot(l);
		shoot(3.2);
		step(10);
		step();
		turnL();
      		turn(2);
		turnR();
		step(see());
	}
}

